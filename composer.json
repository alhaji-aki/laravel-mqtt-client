{
    "name": "alhaji-aki/laravel-mqtt-client",
    "description": "A package to subscribe and publish to mqtt protocols",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "mqtt",
        "iot",
        "publish",
        "subscribe",
        "client"
    ],
    "authors": [
        {
            "name": "Abdul Kudus",
            "email": "abdulkudus2922@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AlhajiAki\\MqttClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlhajiAki\\MqttClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "AlhajiAki\\MqttClient\\MqttClientServiceProvider"
            ],
            "aliases": {
                "MqttClient": "AlhajiAki\\MqttClient\\Facades\\MqttClient"
            }
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "alexmorbo/react-mqtt": "dev-master",
        "illuminate/console": "^7.0|^8.0",
        "illuminate/filesystem": "^7.0|^8.0",
        "illuminate/log": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alhaji-aki/react-mqtt"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
